{
  "name": "mean-stack-blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:ssr:prod": "npm run build:client-and-server-bundles-prod && npm run compile:server",
    "build:client-and-server-bundles-prod": "ng build --configuration production && ng run mean-stack-blog:server:production --bundleDependencies all",
    "build:client-and-server-bundles": "ng build --configuration dev-serv && ng run mean-stack-blog:server:dev-serv --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.8",
    "@angular/common": "~10.0.8",
    "@angular/compiler": "~10.0.8",
    "@angular/core": "~10.0.8",
    "@angular/forms": "~10.0.8",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~10.0.8",
    "@angular/platform-browser-dynamic": "~10.0.8",
    "@angular/platform-server": "~10.0.8",
    "@angular/router": "~10.0.8",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@nguniversal/common": "^10.0.1",
    "@nguniversal/express-engine": "10.0.1",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "bootstrap": "^4.5.1",
    "express": "^4.15.2",
    "ngx-bootstrap": "^5.6.1",
    "ngx-captcha": "^8.0.1",
    "ngx-markdown": "^10.1.0",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.5",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "~10.0.8",
    "@angular/language-service": "~10.0.8",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^7.0.0",
    "ts-loader": "^8.0.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7",
    "webpack-cli": "^3.3.12"
  }
}
